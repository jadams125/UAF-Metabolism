strt.par.2019.Data3 <- strt.par.2019.Data3 %>% filter(DateTime >= "2019-08-01 00:00:00" & DateTime <= "2019-09-01 00:00:00")



strt.par.2019.Data <- rbind(strt.par.2019.Data2, strt.par.2019.Data3, strt.par.2019.Data)



